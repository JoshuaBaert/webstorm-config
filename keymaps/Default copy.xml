<keymap version="1" name="Default copy" parent="$default">
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="CopyElement" />
  <action id="EditorLookupDown" />
  <action id="EditorLookupUp" />
  <action id="EditorScrollDown" />
  <action id="EditorScrollUp" />
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="ctrl up" />
  </action>
  <action id="EditorUnSelectWord">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
    <keyboard-shortcut first-keystroke="ctrl down" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="shift f10" />
    <keyboard-shortcut first-keystroke="f5" />
  </action>
  <action id="RunToCursor">
    <keyboard-shortcut first-keystroke="alt f9" />
    <keyboard-shortcut first-keystroke="alt f5" />
  </action>
  <action id="UML.ApplyCurrentLayout" />
</keymap>